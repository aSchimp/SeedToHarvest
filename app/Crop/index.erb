<div data-role="page">

	<div data-role="header" data-position="inline">
    <h1>Crops - <%= $memoryData.AppSettings.selectedSeasonYear %></h1>
    <a href='/app/Home/gotoIndex' class="ui-btn-left" data-icon="back" data-direction="reverse">Home</a>
    <a href="<%= url_for :action => :new %>" class="ui-btn-right" data-icon="plus">
      New
    </a>
	</div>

  <div data-role="content">
    <ul data-role="listview">
      <% @crops.each do |crop| %>
      
          <li>
            <a href="<%= url_for :action => :showPlantingRecords, :id => crop.object %>">
              <%= crop.CropName %>
            </a>
          </li>
      
      <% end %>
    </ul>
  </div>

</div>
